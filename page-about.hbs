{{!< default}}

{{#post}}

    <header class="page-title">
        <h1 class="page-title-main">{{title}}</h1>
        <p class="page-title-separator"></p>
        <h2 class="page-title-sub">{{{alchemy-postDate published_at}}}</h2>
        <hr />
    </header>

    <main class="content" role="main">
        <article class="{{post_class}}">
            <div class="post-content">

                {{ content }}

                {{{alchemy-socialMediaLinks}}}

            </div>
        </article>
    </main>

{{/post}}
